{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Feries</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ entity.annee|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Paques</th>
                <td>{{ entity.paques|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Ascension</th>
                <td>{{ entity.ascension|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Pentecote</th>
                <td>{{ entity.pentecote|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('feries') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('feries_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
